with uniqueLocation as (
	select lat, lon
	from Insurance
	group by lat, lon
	having count(*) = 1
), validCount as (
	select tiv_2015
	from Insurance
	group by tiv_2015
	having count(*) > 1
)
select Round(sum(i.tiv_2016), 2) as tiv_2016
from Insurance i inner join uniqueLocation u on i.lat = u.lat and i.lon = u.lon
where tiv_2015 in (select * from validCount)
