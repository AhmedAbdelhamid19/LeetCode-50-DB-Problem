select 
    id,
    case 
        when id % 2 = 1 then case when not (total % 2 = 1 and id = total) then lead(student, 1) over (order by id) else student end
        else case when id % 2 = 0 then lag(student,1) over (order by id) end
    end as 'student'
from Seat, (select count(*) as total from Seat) as tot
